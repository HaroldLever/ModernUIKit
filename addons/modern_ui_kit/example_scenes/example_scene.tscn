[sub_resource type="GDScript" id="GDScript_p6q7y"]
resource_name = "ui_controller"
script/source = "extends Control

# 垂直滚动容器的按钮
@export var v_button:Button
# 水平滚动容器的 TextureRect
@export var h_tex_rect:TextureRect
# 滚动容器的 TextureRect
@export var hv_tex_rect:TextureRect

# Called when the node enters the scene tree for the first time.
func _ready():
	if v_button:
		var parent_node = v_button.get_parent()
		for i in 50:
			var new_node = v_button.duplicate()
			var new_name = \"Button\" + str(i+2)
			new_node.name = new_name
			new_node.text = new_name
			parent_node.add_child(new_node)
	if h_tex_rect:
		var parent_node = h_tex_rect.get_parent()
		for i in 20:
			var new_node = h_tex_rect.duplicate()
			var new_col = Color(
				randf_range(0.5, 1.5),
				randf_range(0.5, 1.5),
				randf_range(0.5, 1.5),
				1.0
			)
			new_node.modulate = new_col
			parent_node.add_child(new_node)
	if hv_tex_rect:
		var parent_node = hv_tex_rect.get_parent()
		for i in 200:
			var new_node = hv_tex_rect.duplicate()
			var new_col = Color(
				randf_range(0.5, 1.5),
				randf_range(0.5, 1.5),
				randf_range(0.5, 1.5),
				1.0
			)
			new_node.modulate = new_col
			parent_node.add_child(new_node)


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	pass
"

